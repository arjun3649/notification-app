{
  "compilerOptions": {
    "jsx": "react-native",
    "target": "ES2020",
    "module": "esnext",
    "lib": [
      "ES2020"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "isolatedModules": true,
    "moduleResolution": "node"
  },
  "extends": "expo/tsconfig.base"
}
